
public static int birthday(List<Integer> s, int d, int m) {
    int count = 0;
    for(int i=0;i<=s.size()-m;i++){
        int sum = 0;
        for (int j=i;j<i+m;j++){
            sum = sum + s.get(j);
        }
        if(sum==d){
            count = count + 1;
        }
    }
     return count;
    }
